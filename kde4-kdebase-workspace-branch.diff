+{
+    QDateTime d = QDateTime::currentDateTime();
+    m_date = d.date().day();
+    int updateIn = (24 * 60 * 60) - (d.toTime_t() + KSystemTimeZones::local().currentOffset()) % (24 * 60 * 60);
